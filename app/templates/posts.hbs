<div class="container">

    <section>
    {{#each model as |post|}}
      <div class="col-md-6">
          <div><h3>{{post.title}}</h3> Vote Count: {{post.vote}}</div>
          {{ember-youtube ytid=post.link}}
          <div>{{post.body}}</div>
          <button {{action 'voteUp' post.id}}>Up Vote</button>
          <button {{action 'voteDown' post.id}}>Down Vote</button>
      </div>
    {{/each}}
    </section>


</div>

{{outlet}}
